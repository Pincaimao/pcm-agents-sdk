    "upload:beta": "npm run tsc && npm publish --tag beta",
    "tsc": "tsc -p . --outDir ./dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@stencil/vue-output-target": "^0.10.7",
    "pcm-agents": "^0.6.25"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "typescript": "^5.8.2",
    "vue": "3"
  }
}